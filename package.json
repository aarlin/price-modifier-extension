{
  "name": "price-markup-matrix",
  "version": "1.0.0",
  "description": "Chrome extension for price markup with configurable rates",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "vite-build": "tsc && vite build",
    "build": "node scripts/generate-icons.js && node build.js",
    "dev-build": "node scripts/generate-icons.js && node build.js --dev",
    "format": "biome format --write .",
    "lint": "biome lint .",
    "check": "biome check --apply ."
  },
  "dependencies": {
    "@radix-ui/react-radio-group": "^1.1.3",
    "@radix-ui/react-switch": "^1.0.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^2.2.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.4.1",
    "@types/chrome": "^0.0.319",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "postcss": "^8.4.35",
    "sharp": "^0.33.5",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vite": "^5.1.3"
  }
}
